{
  "manifest_version": 2,
  "name": "Cartoons ad-free",
  "version": "0.1",
  "background": {
    "persistent": false,
    "scripts": ["thecartoons.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.thewatchcartoononline.tv/*"
      ],
      "js": ["thecartoons.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}